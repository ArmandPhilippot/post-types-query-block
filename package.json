{
	"name": "recent-post-types",
	"version": "0.1.0",
	"description": "Display a list of posts including custom post types.",
	"author": "Armand Philippot",
	"license": "GPL-2.0-or-later",
	"homepage": "https://github.com/armandphilippot/recent-post-types-block#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/armandphilippot/recent-post-types-block.git"
	},
	"bugs": {
		"url": "https://github.com/armandphilippot/recent-post-types-block/issues"
	},
	"keywords": [
		"WordPress",
		"plugin",
		"Gutenberg"
	],
	"main": "build/index.js",
	"scripts": {
		"build": "wp-scripts build",
		"format:js": "wp-scripts format-js",
		"init": "gulp initBlock && gulp renameFiles",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"make-pot": "wp i18n make-pot . languages/recent-post-types.pot --exclude=vendor/ --skip-js",
		"packages-update": "wp-scripts packages-update",
		"release": "standard-version -a",
		"start": "wp-scripts start",
		"translate": "gulp translate",
		"zip": "gulp zipBlock"
	},
	"dependencies": {
		"@wordpress/block-editor": "^5.3.0",
		"@wordpress/blocks": "^8.0.0",
		"@wordpress/i18n": "^3.19.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^11.0.0",
		"@commitlint/config-conventional": "^11.0.0",
		"@wordpress/browserslist-config": "^2.7.0",
		"@wordpress/eslint-plugin": "^7.3.0",
		"@wordpress/scripts": "^14.0.1",
		"browserslist": "^4.15.0",
		"del": "^6.0.0",
		"dotenv": "^8.2.0",
		"dotenv-expand": "^5.1.0",
		"eslint": "^7.14.0",
		"eslint-config-prettier": "^6.15.0",
		"gulp": "^4.0.2",
		"gulp-notify": "^3.2.0",
		"gulp-rename": "^2.0.0",
		"gulp-replace": "^1.0.0",
		"gulp-zip": "^5.0.2",
		"husky": "^4.3.0",
		"prettier": "npm:wp-prettier@^2.2.1-beta-1",
		"standard-version": "^9.0.0",
		"stylelint": "^13.8.0",
		"stylelint-config-prettier": "^8.0.2",
		"stylelint-config-wordpress": "^17.0.0",
		"vinyl-paths": "^3.0.1"
	},
	"browserslist": [
		"extends @wordpress/browserslist-config"
	],
	"standard-version": {
		"scripts": {
			"postbump": "gulp bump && git add -A"
		}
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	}
}
